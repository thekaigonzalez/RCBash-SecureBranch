# this is the file containing basic types

# alias to neofetch
set nf `bash -c "neofetch"`

# the prompt
set ps1 "[ rcsh ] $ "

# Agent (REQUIRED)
set agent "sandBOX"

# help command
set help `printf "there are not any help commands.\nYou can learn about the RCSH in the README.\nWe've set the default PS1 to [ rcsh ] #"`

# default shell (set)
set shell /bin/bash

set secure_warnings no

# z shell
set zshell /bin/zsh

# uncomment to enable warnings (if someone's trying to cd into /usr, anything with /system, etc ...) (RECOMMENDED)
#set secure_warnings "yes"

